<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.demo.repository.TestRepository">
    <resultMap id="baseResultMap" type="TestModel">
        <result property="num1" column="num1" jdbcType="INTEGER" />
        <result property="str1" column="str1" jdbcType="VARCHAR" />
    </resultMap>

    <select id="get" parameterType="String" resultType="com.demo.model.TestModel">
        SELECT * FROM users WHERE str1 = #{str1}
    </select>

    <insert id="insert" parameterType="com.demo.model.TestModel">
    INSERT INTO test(str1, num1) VALUES (#{str1}, #{num1})
    </insert>

</mapper>